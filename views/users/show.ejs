<% include ../partials/head %>
<% include ../partials/nav %>
<main>
    <div class="card">
        <img class="card-img-top" src="<%= profile.picture %>">
        <div class="card-body">
            <h5 class="card-title"><%= profile.firstName %> <%= profile.lastName %></h5>
            <h6 class="card-subtitle mb-2 text-muted"><%= profile.status %></h6>
            <a href="mailto:<%= profile.email %>" class="card-link"><%= profile.email %></a>
        </div>
    </div>
</main>
<% include ../partials/footer %>